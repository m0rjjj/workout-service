{
  "name": "workout-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist coverage .nyc_output",
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --inspect --transpileOnly ./src/index.ts",
    "start:prod": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "test": "npm run test:lint && npm run test:unit:coverage",
    "test:unit:coverage": "nyc --reporter=html --reporter=text mocha --recursive",
    "test:unit:watch": "mocha --recursive --watch --watch-extensions ts",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "watch": "run-p start:dev test:unit:watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tsed/common": "5.4.0",
    "@tsed/core": "5.4.0",
    "@tsed/di": "5.4.0",
    "@tsed/swagger": "5.4.0",
    "@tsed/testing": "5.4.0",
    "@tsed/typeorm": "5.4.0",
    "@types/swagger-schema-official": "^2.0.15",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "pg": "^7.9.0",
    "sequelize": "^5.2.0",
    "ts-httpexceptions": "^4.0.0",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.12.0",
    "@types/sinon": "^7.0.10",
    "@types/sinon-chai": "^3.2.2",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "sequelize-typescript": "^1.0.0-alpha.9",
    "sinon": "^7.3.0",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  }
}
